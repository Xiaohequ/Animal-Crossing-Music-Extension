{
   "background": {
      "scripts": [ "src/Utility.js", "src/tune_player.js", "src/TownTuneManager.js", "src/TimeKeeper.js", "src/LoopTimes.js", "src/WeatherManager.js", "src/AudioManager.js", "src/BadgeManager.js", "src/NotificationManager.js", "src/StateManager.js", "src/main.js" ]
   },
   "browser_action": {
      "default_icon": {
		  "38": "assets/img/icon_38_kk_playing.png"
      },
