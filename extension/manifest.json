   "icons": {
      "128": "assets/img/icon_128_kk.png",
      "38": "assets/img/icon_32_kk.png"
   },
   "manifest_version": 3,
   "name": "Animal Crossing Music",
   "options_page": "assets/options.html",
   "permissions": [ 
		"notifications", 
		"storage", 
		"http://api.openweathermap.org/" ],
